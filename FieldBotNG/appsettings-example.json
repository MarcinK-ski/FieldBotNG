{
  "appsettings": {
    "discordBot": {
      "name": "<Optional-bot-Name>",
      "token": "<Bot-Token>",
      "allowedChannels": [
        "<Allowed-channel-id-1>",
        "<Allowed-channel-id-2>",
        "<...>",
        "<Allowed-channel-id-N>"
      ]
    },
    "RemoteHost": {
      "ip": "<Your-Server-URL-or-IP",
      "port": "<Port-Forwarding-Onto>",
      "user": "<Username-For-SSH>",
      "password": "<Password-For-SSH>"
    },
    "LocalHost": {
      "ip": "<Your-local-device-URL-or-IP",
      "port": "<Port-Forwarding-Onto>",
      "isLocalDevice": "true"
    },
    "wsl": "<true-or-false-depends-on-used-enviroment>"
  }
}